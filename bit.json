{
  "env": {
    "compiler": {
      "bit.envs/compilers/react-typescript@3.1.52": {
        "rawConfig": {
          "tsconfig": {
            "compilerOptions": {
              "types": ["node"]
            }
          }
        }
      }
    }
  },
  "componentsDefaultDirectory": "components/{name}",
  "packageManager": "npm",
  "resolveModules": {
    "aliases": {
      "@": "src/components",
      "@style": "src/styles",
      "@utile": "src/utilites",
      "@model": "src/models"
    }
  },
  "overrides": {
    "*": {
      "devDependencies": {
        "@types/node": "^12.12.29"
      }
    }
  }
}
